{
  "extends": "airbnb-base",
  "plugins": [
    "import",
    "vue"
  ],
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "webpack.config.js"
      }
    }
  },
  "rules": {
    "import/extensions": ["error", "always", {
      "js": "never",
      "vue": "never"
    }],
    "comma-dangle": ["error", "always-multiline"]
  }
}
